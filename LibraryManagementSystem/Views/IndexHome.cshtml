@model IEnumerable<LibraryManagementSystem.Models.User>
<h2>Users List</h2>
<table>
    <tr>
        <th>Name</th>
        <th>Email</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Name</td>
            <td>@user.Email</td>
        </tr>
    }
</table>